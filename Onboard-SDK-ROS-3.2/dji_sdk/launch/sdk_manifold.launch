<launch>
	<node pkg="dji_sdk" type="dji_sdk_node" name="dji_sdk" output="screen">
		<!-- node parameters -->
		<param name="serial_name" type="string" value="/dev/ttyUSB0"/>
		<param name="baud_rate" type="int" value="230400"/>
		<param name="app_id" type="int" value="1048114"/>
		<param name="app_version" type="int" value="1"/>
		<param name="app_bundle_id" type="string" value="Not required in this SDK version"/>
		<param name="enc_key" type="string" value="b7eb7bd1ea95805caaa6e5d61aa8e79770ca86a55a0d64b31fb00ee2f44b2ad7"/>
		<param name="groundstation_enable" type="int" value="1"/>
	</node>
</launch>
